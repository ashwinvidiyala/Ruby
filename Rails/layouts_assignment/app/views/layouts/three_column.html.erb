<!DOCTYPE html>
<html>
<head>
  <title>LayoutsAssignment</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="container">
    <div class="row">
      <div class="col-sm-6">
        <%= render 'partials/all_posts' %>
      </div>
      <div class="col-sm-3">
        <%= render 'partials/new_post' %>
      </div>
      <div class="col-sm-3">
        <%= render 'partials/users_page' %>
      </div>
    </div>
  </div>


</body>
</html>
